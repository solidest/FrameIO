//项目:1.test_calc
project test_integer_max_min {
	//受控对象
	system testIntegerMaxMin {
		uint len;
		uint head;
		int end;

		channel CHS : tcpserver {
			serverip = "192.168.0.151";
			port = 8007;
			clientip = "192.168.0.151";
		}
		channel CHC : tcpclient {
			serverip = "192.168.0.151";
			port = 8007;
		}

		action A_Send : send frameSR on CHS {
			HEAD : head;
			LEN : len;
			END:end;
		}
		action A_Recv : recv frameSR on CHC {
			HEAD : head;
			LEN : len;
			END : end;
		}
	}

	//数据帧
	frame frameSR {
		integer HEAD;
		integer LEN;
		integer END max=100 min=5;
	}

}
